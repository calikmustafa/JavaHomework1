package labguestion2;

public class Project2 {

	public static void main(String[] args) {
		
			int speed=70;
			int time1=6;
			int time2=10;
			int time3=15;
			int distance1=speed*time1;
			int distance2=speed*time2;
			int distance3=speed*time3;
			
			System.out.print("The distance the car will travel in 6 hours"+"  "+ distance1 +" \n");
			System.out.print("The distance the car will travel in 10 hours"+" "+ distance2+" \n");
			System.out.print("The distance the car will travel in 15 hours"+" "+ distance3+" \n");
			
	}

}
